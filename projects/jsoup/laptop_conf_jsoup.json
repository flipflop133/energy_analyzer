{
    "config_version": "1.0.0",
    "repo": {
        "url": "https://github.com/jhy/jsoup",
        "branch": "master"
    },
    "execution_plan": {
        "mode": "tests",
        "ignore_failures": true,
        "granularity": "commits",
        "test_command": "mvn surefire:test",
        "num_runs": 15,
        "num_repeats": 1,
        "randomize_tasks": true,
        "batch_size": 100,
        "compile_commands": [
            "mvn clean test-compile"
        ],
        "newest_commit": "5986ca1d8ed0c68a35f42ea0b919d8ec66c3294e",
        "num_commits": 200
    },
    "results": {
        "file": "energy_usage.csv"
    },
    "limits": {
        "temperature_safe_limit": 65000,
        "energy_regression_percent": 20
    },
    "tracked_file_extensions": [
        "java",
        "xml",
        "properties",
        "yaml",
        "yml"
    ],
    "cpu_thermal_file": "/sys/class/hwmon/hwmon5/temp1_input",
    "ignored_directories": [
        "src/test"
    ]
}